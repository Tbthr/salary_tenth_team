<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.salary.mapper.BillItemMapper">
    <resultMap id="BaseResultMap" type="com.salary.model.BillItem">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="item" jdbcType="VARCHAR" property="item"/>
        <result column="flag" jdbcType="INTEGER" property="flag"/>
    </resultMap>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete
        from bill_item
        where id = #{id,jdbcType=INTEGER}
    </delete>
    <insert id="insert" parameterType="com.salary.model.BillItem">
        insert into bill_item (id, item, flag)
        values (#{id,jdbcType=INTEGER}, #{item,jdbcType=VARCHAR}, #{flag,jdbcType=INTEGER})
    </insert>
    <update id="updateByPrimaryKey" parameterType="com.salary.model.BillItem">
        update bill_item
        set item = #{item,jdbcType=VARCHAR},
            flag = #{flag,jdbcType=INTEGER}
        where id = #{id,jdbcType=INTEGER}
    </update>
    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select id, item, flag
        from bill_item
        where id = #{id,jdbcType=INTEGER}
    </select>
    <select id="selectAll" resultMap="BaseResultMap">
        select id, item, flag
        from bill_item
    </select>
</mapper>