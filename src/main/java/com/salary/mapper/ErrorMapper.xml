<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.salary.mapper.ErrorMapper">
    <resultMap id="BaseResultMap" type="com.salary.model.Error">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="error_item" jdbcType="VARCHAR" property="errorItem"/>
    </resultMap>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete
        from error
        where id = #{id,jdbcType=INTEGER}
    </delete>
    <insert id="insert" parameterType="com.salary.model.Error">
        insert into error (id, error_item)
        values (#{id,jdbcType=INTEGER}, #{errorItem,jdbcType=VARCHAR})
    </insert>
    <update id="updateByPrimaryKey" parameterType="com.salary.model.Error">
        update error
        set error_item = #{errorItem,jdbcType=VARCHAR}
        where id = #{id,jdbcType=INTEGER}
    </update>
    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select id, error_item
        from error
        where id = #{id,jdbcType=INTEGER}
    </select>
    <select id="selectAll" resultMap="BaseResultMap">
        select id, error_item
        from error
    </select>
</mapper>